    "@types/chai-as-promised": "^7.1.5",
    "@types/deasync": "^0.1.2",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "*",
    "chai": "^4.3.7",
